#!/home/drahflow/elymas/interpreter/elymas

"standard.ey" include
"elymasGlobal.ey" include
"elymasLexer.ey" include

sys .argv len not { "Usage: ./elymas <input file.ey>" die } rep

[
  "standard.ey"
  "standardClient.ey"
  0 sys .argv *
] {
  sys .file -0010 .open
                  executeFile
                  .close
} each

