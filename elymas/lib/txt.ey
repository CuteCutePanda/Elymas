<
  <
    # 0 -> string
    # 0 <- leading digits converted to int (0, if none)
    { "^(\\d*)" regex -- ==n
      [ n { 48 sub } each ] reverse 10 math .unbase
    } /u deffd
  > /consume defvd

  <
    # 0 -> positive integer
    # 0 <- decimal representation as string
    {
      [ -01 10 math .base reverse "0123456789" each ] str .fromArray
    } /u deffd
  > /produce defvd
> /txt defvd

